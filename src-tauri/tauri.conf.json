{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-config-schema/schema.json",
  "productName": "CreateMyVPN",
  "version": "0.1.0",
  "identifier": "com.createmyvpn.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "CreateMyVPN â€” Your Private VPN",
        "width": 900,
        "height": 650,
        "resizable": true,
        "minWidth": 780,
        "minHeight": 550,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": {
      "wintun.dll": "wintun.dll"
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
